// Size of a single 32x32 bitmap (in bytes)
const size_t BITMAP_SIZE = 32 * 32 / 8; // 128

// Number of bitmaps present
const size_t BITMAP_COUNT = sizeof(epd_bitmaps) / BITMAP_SIZE;

// Access a bitmap by index (0-based). Returns pointer inside PROGMEM array.
const uint8_t* getBitmapByIndex(int index) {
  if (index < 0 || (size_t)index >= BITMAP_COUNT) return nullptr;
  return epd_bitmaps + (index * BITMAP_SIZE);
}

// Access a bitmap by enum IconId for readability
const uint8_t* getBitmapByIcon(IconId id) {
	return getBitmapByIndex(static_cast<int>(id));
}
